		"fabric-ca-client": "^1.4.1",
		"fabric-network": "^1.4.1",
		"lodash": "^4.17.11",
		"morgan": "^1.9.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/register": "^7.4.4",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"eslint": "^5.16.0",
		"esm": "^3.2.25",
